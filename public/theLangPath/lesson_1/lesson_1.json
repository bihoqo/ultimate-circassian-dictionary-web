    "audio": "theLangPath\\lesson_1\\audio\\11.1_3.mp3",
    "parts": [
      {
        "type": "dialogBubble",
        "data": {
          "leftOrRight": "left",
          "characterName": "Синэмис",
          "originText": "Сэ сцlэр Цэй Синэмис. 25 синыбжь.",
          "langToTranslationMap": {
            "en": "My name is Tzei Sinemis. I am 25 years old.",
            "ar": ".أنا اسمي تزي سينميس. عمري 25",
            "he": ".שמי צ'י סינמיס. בת 25"
          }
        }
      },
      {
        "type": "dialogBubble",
        "data": {
          "leftOrRight": "left",
          "characterName": "Синэмис",
          "originText": "Налчыкы сыдэс. Налчык Къэбэртаим икъэлэ шъхьаl.",
          "langToTranslationMap": {
            "en": "I live in Nalchik. Nalchik is the capital of Kabardino-Balkaria.",
            "ar": ".أسكن في نالتشيك. نالتشيك هي عاصمة القبرتاي",
            "he": ".אני גרה בנאלצ'יק. נאלצ'יק היא בירת קברדינו-בלקריה"
          }
        }
      },
      {
        "type": "dialogBubble",
        "data": {
          "leftOrRight": "left",
          "characterName": "Синэмис",
          "originText": "Сэ сыегъэджакlу. Адыгабзэ есэгъаджэх.",
          "langToTranslationMap": {
            "en": "I am a teacher. I teach Adyghe language.",
            "ar": ".أنا معلمة. أعلم الأديغابزة",
            "he": ".אני מורה. מלמדת את השפה האדיגית"
          }
        }
      },
      {
        "type": "dialogBubble",
        "data": {
          "leftOrRight": "left",
          "characterName": "Синэмис",
          "originText": "Адыгабзэр сикlас. Джыри езгъэджэнхэр сикlас.",
          "langToTranslationMap": {
            "en": "I love Adyghe language. I also love teaching.",
            "ar": ".أحب الأديغابزة. وأيضا أحب التعليم",
            "he": ".אני אוהבת את השפה האדיגית .אני גם אוהבת ללמד"
          }
        }
      },
      {
        "type": "dialogBubble",
        "data": {
          "leftOrRight": "left",
          "characterName": "Синэмис",
          "originText": "Сэ сыдэкlуагъ. Пшъашъэ сиl, Синэ ыцlэр, илъэситlу ыныбжь.",
          "langToTranslationMap": {
            "en": "I am married. I have a daughter, her name is Sina, she is two years old.",
            "ar": ".أنا متزوجة. لدي إبنة، اسمها سينا ، عمرها سنتين",
            "he": ".אני נשואה. יש לי בת, שמה סינה, היא בת שנתיים"
          }
        }
      }
    ]
  }
]